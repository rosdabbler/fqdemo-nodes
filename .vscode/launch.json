{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) subpub debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/../../install/${workspaceFolderBasename}/lib/${workspaceFolderBasename}/subpub",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    }
  ]
}
